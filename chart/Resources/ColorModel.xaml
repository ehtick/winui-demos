<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:chart="using:Syncfusion.UI.Xaml.Charts"
    xmlns:coll="using:System.Collections">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="GridBackgroundColor" Color="{StaticResource SystemAltHighColor}"/>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="GridBackgroundColor" Color="Black"/>
        </ResourceDictionary>
        <ResourceDictionary x:Key="HighContrast">
            <SolidColorBrush x:Key="GridBackgroundColor" Color="{ThemeResource SystemColorWindowColor}"/>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>

    <SolidColorBrush x:Key="SeriesColor1" Color="#0078DE"/>
    <SolidColorBrush x:Key="SeriesColor2" Color="#00CC6A"/>
    <SolidColorBrush x:Key="SeriesColor3" Color="#B146C2"/>
    <SolidColorBrush x:Key="SeriesColor4" Color="#FFB900"/>

    <BrushCollection x:Key="CustomBrushes">
        <SolidColorBrush Color="#0078DE" />
        <SolidColorBrush Color="#00CC6A" />
        <SolidColorBrush Color="#B146C2" />
        <SolidColorBrush Color="#FFB900" />
        <SolidColorBrush Color="#7A7574" />
        <SolidColorBrush Color="#E2182F" />
        <SolidColorBrush Color="#7200E6" />
        <SolidColorBrush Color="#007F00" />
        <SolidColorBrush Color="#E3236F" />
        <SolidColorBrush Color="#FA9901" />
    </BrushCollection>

</ResourceDictionary>
